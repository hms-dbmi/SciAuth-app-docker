{
    "family": "sciauth-app",
    "containerDefinitions": [
        {
            "name": "sciauth-app",
            "image": "dbmi/sciauth-app-docker:development",
            "cpu": 1,
            "memory": 200,
            "memoryReservation": 200,
			"portMappings": [
				{
				  "containerPort": 8001,
				  "hostPort": 8001
				} ],
			"environment" : [
				{ "name" : "VAULT_ADDR", "value" : "https://vault.aws.dbmi.hms.harvard.edu:443" },
				{ "name" : "VAULT_SKIP_VERIFY", "value" : "1" },
				{ "name" : "VAULT_PATH", "value" : "secret/dbmi/sciauth" }				
			]				
        }
    ]
}